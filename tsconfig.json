{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"strictNullChecks": true,
		"jsx": "preserve",
		"jsxImportSource": "react",
		"verbatimModuleSyntax": true,
		"paths": {
			"src": ["./src"],
            "@components/*": ["./src/components/*"],
		}
	},
	"include": ["src"],
	"exclude": ["dist"]
}
